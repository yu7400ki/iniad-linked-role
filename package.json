    "@hono/vite-cloudflare-pages": "^0.1.2",
    "@hono/vite-dev-server": "^0.4.0",
    "husky": "^9.0.5",
    "lint-staged": "^15.2.0",
    "vite": "^5.0.12",
    "wrangler": "^3.22.0"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx,json}": [
      "biome check --apply"
    ]
